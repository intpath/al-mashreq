<odoo>
	<data>

		<record id="account_paper_form" model="ir.ui.view">
			<field name="name">account.paper.form</field>
			<field name="model">account.paper</field>
			<field name="arch" type="xml">
				<form string="">
					<sheet>
						<group>						
							<group>						
								<field name="from_date"/>
								<field name="to_date"/>
							</group>	
						</group>
					</sheet>
					<footer class="oe_edit_only"> 
						<button name="%(al-mashreq.account_paper_report_action)d" class="btn btn-primary" type="action" string="Print"/>
						<button string="Discard" special="cancel"/>
					</footer>
				</form>
			</field>
		</record>

	</data>
</odoo>